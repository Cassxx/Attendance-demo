<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:id="@android:id/background">
		<shape >
            <corners android:radius="5dip" />
			<gradient 
			    android:angle="0"
			    android:centerColor="@color/text_999999"
			    android:centerY="0"
			    android:endColor="@color/text_999999"
			    android:startColor="@color/text_999999"/>
        </shape>
    </item>
    <item android:id="@android:id/progress"
			>
		<clip >
            <shape >
                <corners android:radius="5dip" />
				<gradient
				    android:angle="270"
				    android:centerColor="@color/btn_blue"
				    android:centerY="0.45"
				    android:endColor="@color/btn_blue"
				    android:startColor="@color/btn_blue"/>
            </shape>
        </clip>
    </item>

</layer-list>
